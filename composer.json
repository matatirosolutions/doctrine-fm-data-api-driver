{
  "name": "matatirosoln/doctrine-fm-data-api-driver",
  "description": "Use FileMaker via the v17+ Data API as your backend database",
  "type": "library",
  "authors": [
    {
      "name": "Steve Winter",
      "email": "steve@msdev.co.uk"
    }
  ],
  "keywords": [
    "doctrine",
    "dbal",
    "FileMaker"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=8.1.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "doctrine/dbal": "2.*|3.*",
    "doctrine/orm": "2.*",
    "greenlion/php-sql-parser": "^4.0",
    "guzzlehttp/guzzle": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "symfony/yaml": "3.*",
    "sensio/framework-extra-bundle": "3.*",
    "symfony/framework-bundle": "3.*",
    "doctrine/doctrine-bundle": "^1.6",
    "symfony/validator": "3.*"
  },
  "autoload": {
    "psr-4": {
      "MSDev\\DoctrineFMDataAPIDriver\\": ""
    }
  }
}
